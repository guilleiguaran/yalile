<div class="box-default head-table clearfix">
  <%= search_form_for @search, url: reports_path, html: {method: :get} do |form| %>
    <div class="four-fieldset clearfix more-separate">
      <fieldset class="datepicker">
        <%= form.label :created_at_gteq, content_tag(:strong, "Fecha Inicial") %>
        <%= form.text_field :created_at_gteq, class: "datepicker text", placeholder: "DD/MM/YYYY" %>
      </fieldset>
      <fieldset class="datepicker">
        <%= form.label :created_at_lteq, content_tag(:strong, "Fecha Final") %>
        <%= form.text_field :created_at_lteq, class: "datepicker text", placeholder: "DD/MM/YYYY" %>
      </fieldset>
      <fieldset class="btn-alone last">
        <%= content_tag :div, form.submit("Buscar", name: nil), class: "btn-green" %>
      </fieldset>
    </div><!--.four-fieldset-->
  <% end %>
</div>
<!-- defaults: {wrapper_html: {class: :datepicker}, input_html: {class: "datepicker text", maxlength: 8, readonly: true}} -->