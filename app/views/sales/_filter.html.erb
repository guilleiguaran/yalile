<div class="box-default clearfix head-table box-app">
  <%= search_form_for @search do |form| %>
    <div class="four-fieldset clearfix">
      <fieldset>
        <%= form.label :user_username_cont, "Vendedor:" %>
        <%= form.text_field :user_username_cont, class: :text %>
      </fieldset>
      <fieldset>
        <%= form.label :total_gteq, "Valor Desde:" %>
        <%= form.text_field :total_gteq, class: :text %>
      </fieldset>
      <fieldset>
        <%= form.label :total_lteq, "Valor Hasta:" %>
        <%= form.text_field :total_lteq, class: :text %>
      </fieldset>
      <fieldset class="btn-alone last">
        <%= content_tag :div, form.submit("Buscar", name: nil), class: "btn-green" %>
      </fieldset>
    </div><!-- box-search-filter clearfix END -->
    <div class="four-fieldset clearfix">
      <fieldset>
        <%= form.label :created_at_gteq, "Desde:" %>
        <%= form.text_field :created_at_gteq, class: "datepicker text", placeholder: "DD / MM / YYYY", readonly: true %>
      </fieldset>
      <fieldset>
        <%= form.label :created_at_lteq, "Hasta:" %>
        <%= form.text_field :created_at_lteq, class: "datepicker text", placeholder: "DD / MM / YYYY", readonly: true %>
      </fieldset>
    </div><!-- box-search-filter clearfix END -->
  <% end %>
</div><!-- head-table box-default clearfix END -->